<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >
     <TableLayout
          
             android:layout_width="fill_parent"
             android:layout_height="wrap_content"
             android:stretchColumns="2" >


    <TableRow>

        <TextView
            android:id="@+id/lblregambassadorname"
            android:layout_column="1"
            android:layout_weight="2"
            android:padding="3dip"
            android:text="Ambassador Name:"
            tools:ignore="HardcodedText" />

        <EditText
            android:id="@+id/txtregambassadorname"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="textPersonName" >

            <requestFocus />
        </EditText>

    </TableRow>


    <TableRow >

                <TextView
                    android:id="@+id/lblphysicallocation"
                    android:layout_column="1"
                    android:layout_weight="2"
                    android:padding="3dip"
                    android:text="Physical Location:"
                    tools:ignore="HardcodedText" />

                <EditText
                    android:id="@+id/txtphysicallocation"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:ems="10"
                    android:inputType="textPersonName" />

    </TableRow>
<TableRow>

    <TextView
        android:id="@+id/lblphonecontact"
        
        android:layout_column="1"
        
        android:layout_weight="2"
        android:gravity="left"
        android:padding="3dip"
        android:text="Phone Contact:"
        tools:ignore="HardcodedText" />

    <EditText
        android:id="@+id/txtphonecontact"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="phone" />
    
    </TableRow>

  <TableRow>

            <TextView
                android:id="@+id/lbllregdate"
                android:layout_column="1"
                android:layout_weight="2"
                android:gravity="left"
                android:padding="3dip"
                android:text="Registration Date"
                tools:ignore="HardcodedText" />
       
    </TableRow>

    

  
    <TableRow>

          <TextView
              android:id="@+id/lblrelationprofessional"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_column="1"
              android:layout_weight="2"
              android:padding="3dip"
              android:text="Relation Or Professional:"
              tools:ignore="HardcodedText" />

          <EditText
              android:id="@+id/txtrelationprofessional"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:ems="10"
              android:inputType="textPersonName" />

</TableRow>
<TableRow >
 <TextView
                    android:id="@+id/lblreporterto"
                    android:layout_column="1"
                    android:layout_weight="2"
                    android:padding="3dip"
                    android:text="Supporter To:"
                    tools:ignore="HardcodedText" />
    <Spinner
                    android:id="@+id/spinnerreporterto"
                    android:layout_column="1"
                    android:layout_weight="2"
                    android:padding="3dip"/>
    
</TableRow>
     <TableRow>
<TextView
                android:id="@+id/lblambarGPSlat"
                android:layout_column="1"
                android:layout_weight="2"
                android:gravity="left"
                android:padding="3dip"
                android:text="GPS Latitude Coordinates:"
                tools:ignore="HardcodedText" />

<TextView
    android:id="@+id/lblambassadorGPSlat"
    android:layout_column="1"
    android:layout_weight="2"
    android:padding="3dip"
    tools:ignore="HardcodedText" />

    </TableRow>
        <TableRow>
<TextView
                android:id="@+id/lblambarGPSlong"
                android:layout_column="1"
                android:layout_weight="2"
                android:gravity="left"
                android:padding="3dip"
                android:text="GPS Longitude Coordinates:"
                tools:ignore="HardcodedText" />

<TextView
    android:id="@+id/lblambassadorGPSlong"
    android:layout_column="1"
    android:layout_weight="2"
    android:padding="3dip"
    tools:ignore="HardcodedText" />

    </TableRow>

</TableLayout>
</ScrollView>